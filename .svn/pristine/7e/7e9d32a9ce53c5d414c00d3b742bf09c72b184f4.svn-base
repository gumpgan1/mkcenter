@charset "utf-8";
*{
	margin: 0;
	font-family: "微软雅黑", "黑体", Georgia, "Times New Roman", Times, serif;
}
.logo {
background-color: rgba(51,51,51,0.3);	padding-top: 10px;
}
.logo p {
	font-family: "微软雅黑", "黑体", Georgia, "Times New Roman", Times, serif;
	padding-bottom: 15px;
	font-size: 20px;
	font-weight: bold;
}
.over {
	background-image: url(../over.jpg);
	height: 79.5%;
	background-position: center,center;
	background-repeat: space;
	position:relative;
}
.setbar {
	font-family: "微软雅黑", Georgia, "Times New Roman", Times, serif;
	position: fixed;
	padding:10px 5px 10px 5px;
	right: 30px;
	top:10px;
	z-index: 99;
	text-decoration: none;
	background-color:inherit;	
}

.setbar a{
	text-decoration:none;
	color:rgba(238,238,238,1);
	padding:8px;
}
.setbar a:hover{
	color:rgba(0,153,255,1);
	text-decoration:underline;
}
.over .window{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	background-color: rgba(0,0,0,0.40);
	height:210px;
}
.over .window .word{
	background:url(../xibiao.png) left center no-repeat;
	width:700px;
	margin:0 auto;
	padding-left:150px;
}

.over .window .word h3{
	color:rgba(255,255,255,1);
	padding-top:10px;
	padding-bottom:5px;
}

.over .window .word p{
	padding-top:5px;
	color:rgba(255,255,255,1);
	line-height:25px;
	text-indent:30px;
}


.copyright {
	background-color: rgba(0,0,0,1);
	text-align:center;
	padding:20px;
	
}

.copyright p{
	color:rgba(255,255,255,1);
}

.ui-dialog{ 
    width: 380px;height: auto;
    position: absolute;z-index: 9000;
    top: 0px;left: 0px;
    border: 1px solid #D5D5D5;
	background:rgba(10, 10, 10, 0.8);
	display:none;
	position:fixed;
}
 
.ui-dialog a{
	text-decoration: none;
	color:rgba(238,238,238,1);
	
	}
	
	.ui-dialog a: visited{
	color:rgba(238,238,238,1);
}
 
